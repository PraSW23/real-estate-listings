{
    "version": 2,
    "builds": [
      { "src": "frontend/package.json", "use": "@vercel/static-build" },
      { "src": "backend/**/*", "use": "@vercel/node" }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/(.*)",
        "dest": "backend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }
  